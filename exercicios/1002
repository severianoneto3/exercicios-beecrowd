import java.io.IOException;
import java.util.Scanner;

public class Main {
 
    public static void main(String[] args) throws IOException {
        Scanner tec = new Scanner(System.in);
		    double R = tec.nextDouble();
		    double pi = 3.14159;
        double A = pi*R*R;
        System.out.printf("A=" + "%.4f\n",A);
        tec.close();
 
    }
 
}
