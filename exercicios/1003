import java.io.IOException;
import java.util.Scanner;

public class Main {
 
    public static void main(String[] args) throws IOException {
        Scanner tec = new Scanner(System.in);
        int A = tec.nextInt();
        int B = tec.nextInt();
        int SOMA = A+B;
        System.out.println("SOMA = " + SOMA);
        tec.close();
 
    }
 
}
